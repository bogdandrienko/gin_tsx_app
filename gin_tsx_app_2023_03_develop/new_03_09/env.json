{
  "HostGin":  "0.0.0.0",
  "PortGin":  8083,
  "DebugGin": true,
  "PrintErr": true,
  "LogFile":  "log.txt",
  "CacheLow":  5,
  "CacheMiddle":  10,
  "CacheHigh":  30,

  "Cors": {
    "AllowHeaders": ["*"],
    "AllowMethods": ["*"],
    "AllowOrigins": ["*"],
